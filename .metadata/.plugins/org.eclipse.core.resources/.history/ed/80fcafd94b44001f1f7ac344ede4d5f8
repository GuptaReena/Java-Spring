package oops;


class AreaCalculator{
	
	public static double calculateArea(double r) {
		
		double area = 3.14*r*r;
		
		return area;
	}
	
	public static double calculateArea(double l , double b) {
		
		int area = l*b;
		
		return l*b;
	}
	
	public static double calculateArea(double b,double h) {
		
		return 0.5*b*h;
	}
	
}



