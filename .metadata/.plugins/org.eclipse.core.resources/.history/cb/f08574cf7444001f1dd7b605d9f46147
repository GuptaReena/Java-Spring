package oops;


 interface Vehicle{
	
	 void start();
	 void stop();
	 String getType();
}

 class Car implements Vehicle{
	 
	 String brand;
	 String model;
	 
	 

	@Override
	public void start() {
		System.out.println("Car Start using keys");
		
	}

	@Override
	public void stop() {
		System.out.println("Car stop using break");
		
	}

	@Override
	public String getType() {
		// TODO Auto-generated method stub
		
		String s = "Four Wheeler";
		
		return s;
	}
	
	void displayDetails() {
		System.out.println("Cars Brand : " + brand);
		System.out.println("Card Model : " + model);
	}
	 
 }

 class Bicycle implements Vehicle{
		
		int gearCount;

		
		
		public Bicycle(int gearCount) {
			super();
			this.gearCount = gearCount;
		}

		@Override
		public void start() {
			System.out.println("Bicycle run using pedals");
			
		}

		@Override
		public void stop() {
			System.out.println("Bicycle stop by break");
			
		}

		@Override
		public String getType() {
			// TODO Auto-generated method stub
			return null;
		}
		
		void displayDetails() {
			System.out.println("Bicycle Gear Count : " + gearCount);
		}
	}



public class InterfaceExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Car car = new Car();
		car.brand = "Toyata";
		car.model = "ES-2023";
		car.getType(); 
		car.start();
		car.stop();
		car.displayDetails();
		
		Bicycle bicycle = new Bicycle();
		bicycle.gearCount = 15;
		bicycle.start();
		bicycle.stop();
		bicycle.displayDetails();
		
		
		
		
	}

}

