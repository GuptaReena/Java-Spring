package oops;

import java.util.Scanner;

public class Scanner1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc = new Scanner(System.in);
		
		System.out.println("Enter a Line :");
		String s =  sc.next();
		
		
		if(sc.hasNext()) {
			System.out.println(s);
		}
		
	}

}
