package oops;


class AreaCalculator{
	
	public static double calculateArea(double r) {
		
		return Math.PI*r*r;
	}
	
		double calculateArea(double l , double b) {
		
		
		
		return l*b;
	}
	
		double calculateArea(double b,double h) {
		
		return 0.5*b*h;
	}
	
}



