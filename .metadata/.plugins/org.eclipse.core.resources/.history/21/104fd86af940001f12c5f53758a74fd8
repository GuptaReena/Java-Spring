package creationaldesignpattern.Builderpattern;

public class Employee3 {

	int empId;
	String empName;
	String empAddress;
	
	
	
		
		
	
	public static class Builder() {
		
	}

	public Employee3 setEmpId(int empId) {
		this.empId = empId;
		return this;
	}
	
	public Employee3 setEmpName(String empName) {
		this.empName = empName;
		return this;
	}
	
	public Employee3 setEmpAddress(String empAddress) {
		this.empAddress = empAddress;
		return this;
	}
	@Override
	public String toString() {
		return "Employee3 [empId=" + empId + ", empName=" + empName + ", empAddress=" + empAddress + "]";
	}
	
	
	
}
