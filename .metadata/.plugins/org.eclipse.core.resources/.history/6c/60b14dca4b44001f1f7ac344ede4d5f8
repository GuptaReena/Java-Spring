package oops;


class AreaCalculator{
	
	public static double calculateArea(int r) {
		
		double area = 3.14*r*r;
		
		return area;
	}
	
	public static double calculateArea(int l , int b) {
		
		int area = l*b;
		
		return area;
	}
	
}



